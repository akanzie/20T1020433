
@{
    
}
<div class="box box-primary">
    <div class="box-body">
        <form class="form-horizontal" action="">
            <div class="form-group">
                <label class="control-label col-sm-2">Tên nhà cung cấp:</label>
                <div class="col-sm-10">
                    <input type="email"
                           class="form-control"
                           id="SupplierName"
                           name="SupplierName"
                           placeholder="">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Tên giao dịch:</label>
                <div class="col-sm-10">
                    <input type="text"
                           class="form-control"
                           id="ContactName"
                           placeholder="">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Địa chỉ:</label>
                <div class="col-sm-10">
                    <input type="text"
                           class="form-control"
                           id="Address"
                           placeholder="">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Điện thoại:</label>
                <div class="col-sm-10">
                    <input type="text"
                           class="form-control"
                           id="Phone"
                           placeholder="">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Quốc gia:</label>
                <div class="col-sm-10">
                    <select class="form-control" name="Country">
                        <option selected>--Chọn quốc gia--</option>
                        <option value="1">Việt Nam</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-default">Submit</button>
                </div>
            </div>
        </form>
    </div>
</div>

